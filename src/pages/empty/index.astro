---
import BaseHead from '../../components/BaseHead.astro';
import Header from '../../components/Header.astro';
import Footer from '../../components/Footer.astro';

const title = 'empty';
---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={title} description={title} />
	</head>
	<body>
		<Header />
		<main class="page" aria-label="Empty desktop">
			<figure class="page__figure">
				<img
					src="/d.jpg"
					alt="Dog with bared teeth wearing a jeweled collar"
					loading="lazy"
					decoding="async"
					width="236"
					height="389"
				/>
			</figure>
		</main>
		<Footer />

		<style>
			.page {
				min-height: 100vh;
				padding: 90px 28px 84px 28px;
			}
			.page__figure {
				margin: 0;
				max-width: 320px;
			}
			.page__figure img {
				display: block;
				width: 100%;
				height: auto;
				object-fit: cover;
			}
		</style>
	</body>
</html>
